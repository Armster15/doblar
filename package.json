{
  "name": "doblar",
  "version": "0.0.0",
  "scripts": {
    "start": "concurrently \"npm run watch-sw\" \"vite --host\"",
    "build": "npm run build-sw && vite build",
    "serve": "http-server dist",
    "watch-sw": "npx esbuild ./src/imagemagick-worker/sw.ts --outfile=./public/imagemagick_sw.js --watch",
    "build-sw": "npx esbuild ./src/imagemagick-worker/sw.ts --outfile=./public/imagemagick_sw.js --minify",
    "clean": "rimraf dist ./src/module-workers-polyfill.js ./public/imagemagick_sw.js"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@khanacademy/tota11y": "^0.2.0",
    "assert": "^2.0.0",
    "byte-base64": "^1.1.0",
    "classnames": "^2.3.1",
    "comlink": "^4.3.1",
    "concurrently": "^7.0.0",
    "imagemagick-wasm-builds": "github:Armster15/imagemagick-wasm-builds",
    "jotai": "^1.6.0",
    "module-workers-polyfill": "^0.3.2",
    "pretty-ms": "^7.0.1",
    "react": "^17.0.0",
    "react-aria": "^3.13.2",
    "react-dom": "^17.0.0",
    "react-icons": "^4.3.1",
    "react-portal": "^4.2.1",
    "react-select": "^5.2.2",
    "react-use": "^17.3.2"
  },
  "devDependencies": {
    "@imagemagick/magick-wasm": "^0.0.10",
    "@tailwindcss/typography": "^0.5.2",
    "@types/assert": "^1.5.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-portal": "^4.0.4",
    "@vitejs/plugin-react": "^1.2.0",
    "esbuild": "^0.14.23",
    "http-server": "^14.1.0",
    "react-html-props": "^1.0.32",
    "tailwindcss": "^3.0.23",
    "tailwindcss-children": "^2.1.0",
    "type-fest": "^2.12.0",
    "typescript": "^4.3.2",
    "vite": "^2.5.4",
    "vite-plugin-pwa": "^0.11.13"
  }
}
